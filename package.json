  "name": "Proyecto Mantagora",
  "scripts": {
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "build:css": "node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js --output-style compressed scss/estilos.scss css/main.css"
  },
  "version": "1.0.0",
  "devDependencies": {
    "node-sass": "^4.14.1",
    "postcss-cli": "^6.1.3",
    "node-sass-glob-importer": "^5.3.2"
  }
}
